<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
  
<configuration>
	
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
		<setting name="callSettersOnNulls" value="true"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.northstar.bi.dto.Emp" alias="Emp" />
		<typeAlias type="com.northstar.bi.dto.Project" alias="Project"/>
		<typeAlias type="com.northstar.bi.dto.Company" alias="Company"/>
		<typeAlias type="com.northstar.bi.dto.Board" alias="Board"/>
		<typeAlias type="com.northstar.bi.dto.BoardFile" alias="BoardFile"/>
		<typeAlias type="com.northstar.bi.dto.BoardPagination" alias="BoardPagination"/>
		<typeAlias type="com.northstar.bi.dto.BoardCriteria" alias="BoardCriteria"/>
		<typeAlias type="com.northstar.bi.dto.BoardReply" alias="BoardReply"/>
		<typeAlias type="com.northstar.bi.dto.Criteria" alias="Criteria"/>
		<typeAlias type="com.northstar.bi.dto.Customer" alias="Customer"/>
		<typeAlias type="com.northstar.bi.dto.Category" alias="Category"/>
		<typeAlias type="com.northstar.bi.dto.Request" alias="Request"/>
		<typeAlias type="com.northstar.bi.dto.ConPro" alias="ConPro"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="META-INF/mybatis/mappers/emp.xml"/>
		<mapper resource="META-INF/mybatis/mappers/project.xml"/>
		<mapper resource="META-INF/mybatis/mappers/company.xml"/>
		<mapper resource="META-INF/mybatis/mappers/board.xml"/>
		<mapper resource="META-INF/mybatis/mappers/boardReply.xml"/>
		<mapper resource="META-INF/mybatis/mappers/file.xml"/>
		<mapper resource="META-INF/mybatis/mappers/category.xml"/>
		<mapper resource="META-INF/mybatis/mappers/request.xml"/>
	</mappers>
	
</configuration>